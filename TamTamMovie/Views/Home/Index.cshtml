@model IEnumerable<TTMovieModel.Model.Movie>
    
 
<div class="jumbotron">
    <h1>TamTam Movies</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS, and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>



<div class="row">

    @foreach (var item in Model)
    {

        <div class="col-md-5">
            <h2>@Html.DisplayFor(modelItem => item.Title.Name)</h2>
            <p> @Html.DisplayFor(modelItem => item.ID) </p>
            <p><strong>Year</strong> @Html.DisplayFor(modelItem => item.Year) </p>
            <p>
                @Html.DisplayFor(modelItem => item.Resume)
            </p>
            <p><a class="btn btn-default">Ver trailer</a></p>
        </div>
    }


    
</div>
